generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  iduser Int @id @default(autoincrement())
  username String @unique
  password String
  description String
  email String
  idfollow Int @default(autoincrement())
  perfil_image Json?
  social_media Json?
  last_modification DateTime
  creation_date DateTime
  disabled Boolean
}